{
  "name": "lab_reporter",
  "version": "1.0.0",
  "description": "lab_reporter",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev nodemon src",
    "start:debug": "cross-env NODE_ENV=dev node --inspect=0.0.0.0 --inspect-brk src",
    "prod": "node src",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:debug:watch": "cross-env NODE_ENV=test node --inspect=0.0.0.0 --inspect-brk ./node_modules/.bin/jest --watch --no-cache --runInBand",
    "test:debug": "cross-env NODE_ENV=test node --inspect=0.0.0.0 --inspect-brk ./node_modules/.bin/jest --no-cache --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "createuser": "node bin/createuser"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "@types/jest": "^27.4.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "googleapis": "^109.0.1",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:import/recommended",
      "plugin:jest/recommended"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "eqeqeq": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "testTimeout": 15000
  }
}
